# Provider de emissão:
# - portal (RPA/Playwright)
# - nacional (API Emissor Nacional)
NFSE_PROVIDER=portal

# URLs do portal (RPA)
NFSE_PORTAL_URL=https://www.nfse.gov.br/EmissorNacional/Login?ReturnUrl=%2fEmissorNacional
NFSE_HOME_URL=https://www.nfse.gov.br/EmissorNacional
NFSE_EMITIDAS_URL=https://www.nfse.gov.br/EmissorNacional/Notas/Emitidas
NFSE_RECEBIDAS_URL=https://www.nfse.gov.br/EmissorNacional/Notas/Recebidas
NFSE_EMISSAO_URL=https://www.nfse.gov.br/EmissorNacional/DPS/Pessoas

# Playwright (dev)
NFS_HEADLESS=0

# ---------------------------
# API Emissor Nacional (mTLS)
# ---------------------------
# Base URL do ambiente (ajuste quando você confirmar o endpoint certo)
NFSE_NACIONAL_BASE_URL=https://www.nfse.gov.br

# Paths (se o ambiente usar paths diferentes, ajuste aqui sem mexer no código)
NFSE_NACIONAL_PATH_EMITIR=/api/v1/nfse
NFSE_NACIONAL_PATH_CONSULTAR=/api/v1/nfse
NFSE_NACIONAL_PATH_EVENTOS=/api/v1/nfse

# Certificado A1 (.pfx)
NFSE_CERT_PFX_PATH=./certs/empresa.pfx
NFSE_CERT_PFX_PASS=senha_do_pfx
